    "mongoose": "4.10.6",
    "morgan": "1.8.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
